<div class="wrapper">
<h2>My todo app</h2>
<nav class="auth-links">
  <ul>
    <li>
      <%= link_to_unless_current "Log in", new_session_path(resource_name) %>
    </li>
    <li>
      <%= link_to_unless_current "Sign up", new_registration_path(resource_name) %>
    </li>
  </ul>
</nav>
<div class="log-in-form">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div>
    <%= f.email_field :email, autofocus: true, placeholder: "Choose e-mail" %>
  </div>

  <div>
    <%= f.password_field :password, autocomplete: "off", placeholder: "Choose password" %>
  </div>
<!-- 
  <% if devise_mapping.rememberable? -%>
    <div><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
  <% end -%>

 -->  <div><%= f.submit "Log in" %></div>
<% end %>
</div>
